{
    "name": "crescat/saloon-sdk-generator",
    "description": "Convert Postman Collections to PHP SDKs using Saloon.",
    "keywords": [
        "sdk generator",
        "saloon"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Helge Sverre",
            "email": "helge.sverre@crescat.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel-zero/framework": "^10.0.2",
        "nette/php-generator": "^4.0",
        "nunomaduro/termwind": "^1.15.1"
    },
    "require-dev": {
        "laravel/pint": "^1.8",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "codegen"
    ]
}
